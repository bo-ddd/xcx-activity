/* pages/entryProcess/index.wxss */
.entry-box{
    height: 100vh;
}
.entry{
    height: 100%;
}
.red-bg{
    height: 400rpx;
    background: linear-gradient(to right bottom, rgb(243, 211, 211), #e64430);
    
}
.settle{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  
}
.pople{
    width: 350rpx;
    height: 250rpx;
   
}
.cloud{
    width: 100%;
    height: 150rpx;
}
.icon-p{
    width: 50rpx;
    height: 50rpx;
    padding: 10rpx;
    background-color: rgba(252, 216, 220, 0.966);
    border-radius: 20rpx;
    border-radius: 20rrgba(240, 20, 45, 0.966)
}
.entry-modul{
    display: flex;
    justify-content: center;
}
.entry-modul_item{
    display: flex;
    align-items: center;

    padding: 20rpx;
}
.next-btn{
    display: inline-block;

    color: #fff;
    font-weight: bold;
    padding: 15rpx 30rpx;
    border-radius: 30rpx;
    background: linear-gradient(to right bottom, rgb(243, 211, 211), #e45e4c) ;
}
.btn{
    display: flex;
    justify-content: center;
}